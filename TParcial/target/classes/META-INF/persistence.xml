<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="TParcial"
		transaction-type="JTA">
		<jta-data-source>java:/MySqlDS2</jta-data-source>
		<class>pe.edu.upc.entity.Abogado</class>
		<class>pe.edu.upc.entity.Categoria_Producto</class>
		<class>pe.edu.upc.entity.Categoria</class>
		<class>pe.edu.upc.entity.Declaracion_aduanera</class>
		<class>pe.edu.upc.entity.DetalleProducto</class>
		<class>pe.edu.upc.entity.Especialidad</class>
		<class>pe.edu.upc.entity.Flete</class>
		<class>pe.edu.upc.entity.Marca</class>
		<class>pe.edu.upc.entity.ProductoImportar</class>
		<class>pe.edu.upc.entity.Requisito_legal</class>
		<class>pe.edu.upc.entity.Transportista</class>
		<class>pe.edu.upc.entity.ProductoEmpresa</class>
		<class>pe.edu.upc.entity.LineadePedido</class>
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
		<properties>
			<property
				name="javax.persistence.schema-generation.database.action"
				value="update"/>
		</properties>


	</persistence-unit>
</persistence>
